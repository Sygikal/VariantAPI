{
    "schemaVersion": 1,
    "id": "variantapi",
    "version": "${version}",
    "name": "VariantAPI",
    "description": "API for adding variant skins and overlays to mobs",
    "authors": [
        "Sygikal"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/Sygikal/VariantAPI"
    },
    "license": "CC0-1.0",
    "icon": "assets/variantapi/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "dev.sygii.variantapi.VariantAPI"
        ],
        "client": [
            "dev.sygii.variantapi.VariantAPIClient"
        ]
    },
    "accessWidener": "${aw}",
    "mixins": [
        "variantapi.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.16.14",
        "minecraft": "${mcdep}",
        "java": "${javadep}",
        "fabric-api": "*"
    },
    "suggests": {
        "another-mod": "*"
    }
}